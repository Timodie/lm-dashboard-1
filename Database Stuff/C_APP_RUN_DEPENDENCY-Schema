CREATE TABLE C_APP_RUN_DEPENDENCY (
run_app_dependency_id INT PRIMARY KEY NOT NULL,
app_name TEXT NOT NULL,
run_name TEXT NOT NULL,
dependent_app_name TEXT NOT NULL,
dependent_run_name TEXT NOT NULL,
create_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
last_modified_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
app_run_id INT NOT NULL,
dependent_app_run_id INT NOT NULL,
PRIMARY KEY (run_app_dependency_id)
);
