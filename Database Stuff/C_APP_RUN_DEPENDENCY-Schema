CREATE TABLE C_APP_RUN_DEPENDENCY (
Run App Dependency ID INT PRIMARY KEY NOT NULL,
APP Name TEXT NOT NULL,
Run Name TEXT NOT NULL,
Dependant APP Name TEXT NOT NULL,
Dependant Run Name TEXT NOT NULL,
Create Date Time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
Last Modified Date Time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
APP Run ID INT NOT NULL,
Dependant APP Run ID INT NOT NULL,
PRIMARY KEY (Run App Dependency ID)
);
