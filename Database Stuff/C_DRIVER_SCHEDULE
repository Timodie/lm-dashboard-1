CREATE TABLE C_DRIVER_SCHEDULE (audit_id INT PRIMARY KEY NOT NULL, app_name TEXT NOT NULL, run_name TEXT NOT NULL, run_number INT NOT NULL, re_rum_number INT NOT NULL, scheduled_start_date_time TIMESTAMP NOT NULL, status_code CHAR(5) NOT NULL, valuation_start_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, valuation_end_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, run_start_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, run_end_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, create_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, last_modified_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, app_run_id INT, sla_date TIMESTAMP DEFAULT CURRENT_TIMESTAMP, sla_time TIMESTAMP DEFAULT CURRENT_TIMESTAMP);
