CREATE TABLE C_DRIVER_STEP_DETAIL (
Driver Step Detail ID INT NOT NULL,
Audit ID,Driver Step ID INT NOT NULL,
APP Name TEXT NOT NULL,
Run Name TEXT NOT NULL,
Group Number INT NOT NULL,
Run Order Number INT NOT NULL,
*Run Status Code TEXT NOT NULL,
Error Process Number INT NOT NULL,
SESSION START DATE TIME,
SESSION END DATE TIME,
Run Start Date Time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
Run End Date Time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
Create Date Time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
Last Modified Date Time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
PRIMARY KEY (Driver Step Detail ID)
);
