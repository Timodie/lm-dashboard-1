CREATE TABLE C_DRIVER_STEP_DETAIL (
driver_step_detail_id INT NOT NULL,
audit_id INT NOT NULL,
driver_step_id INT NOT NULL,
app_name TEXT NOT NULL,
run_name TEXT NOT NULL,
group_number INT NOT NULL,
run_order_number INT NOT NULL,
run_status_code TEXT NOT NULL,
error_process_number INT NOT NULL,
session_start_date_time TIMESTAMP NOT NULL,
session_end_date_time TIMESTAMP NOT NULL,
run_start_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
run_end_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
create_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
last_modified_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
PRIMARY KEY (driver_step_detail_id)
);
