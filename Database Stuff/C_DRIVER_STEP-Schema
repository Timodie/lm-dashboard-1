CREATE TABLE C_DRIVER_SCHEDULE (
driver_step_id INT NOT NULL,
app_name TEXT NOT NULL,
run_name TEXT NOT NULL,
group_number INT NOT NULL,
group_name TEXT NOT NULL,
run_order_number INT NOT NULL,
path_text TEXT NOT NULL,
command_text TEXT NOT NULL,
parameter_text TEXT NOT NULL,
group_concurrency_indicator TEXT NOT NULL,
step_concurrency_indicator TEXT NOT NULL,
notify_text TEXT NOT NULL,
step_type_code TEXT NOT NULL,
step_name TEXT NOT NULL,
error_process_number INT NOT NULL,
create_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
last_modified_date_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
app_run_id INT NOT NULL,
active_step_indicator TEXT NOT NULL,
PRIMARY KEY (driver_step_id)
);
